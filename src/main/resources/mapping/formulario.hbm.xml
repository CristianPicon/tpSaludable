<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.utn.modelo.Formulario" table="forma" catalog="base__saludable">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="tipcomida" type="String">
			<column name="tipcomida" length="10" not-null="false" />
		</property>
		<property name="comPrincipal" type="String">
			<column name="comPrincipal" length="10" not-null="false" />
		</property>
		<property name="comSecundario" type="String">
			<column name="comSecundario"  length="11" not-null="false" />
		</property>
		<property name="bebida" type="String">
			<column name="bebida" length="11" not-null="false" />
		</property>
		<property name="postre" type="String">
			<column name="postre" length="11" not-null="false" />
		</property>
		<property name="tentacion" type="String">
			<column name="tentacion" length="11" not-null="false" />
		</property>
		
		<property name="fecha" type="java.util.Date">
			<column name="fecha" length="11" not-null="true" />
		</property>
	</class>
</hibernate-mapping>