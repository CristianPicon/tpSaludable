<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.utn.modelo.Auto" table="users" catalog="base__saludable">
		<id name="id_users" type="java.lang.Integer">
			<column name="id_users" />
			<generator class="identity" />
		</id>
		<property name="nombre" type="String">
			<column name="nombre" length="10" not-null="false" />
		</property>
		<property name="apellido" type="String">
			<column name="apellido" length="10" not-null="false" />
		</property>
		<property name="dni" type="java.lang.Integer">
			<column name="dni"  length="11" not-null="false" />
		</property>
		<property name="sexo" type="String">
			<column name="sexo" length="11" not-null="false" />
		</property>
		<property name="fnac" type="java.util.Date">
			<column name="fnac" length="11" not-null="true" />
		</property>
		
		<property name="localida" type="String">
			<column name="localida" length="11" not-null="false" />
		</property>
		<property name="username" type="String">
			<column name="username" length="11" not-null="false" />
		</property>
		
		<property name="password" type="String">
			<column name="password" length="11" not-null="false" />
		</property>
		
		<property name="enable" type="java.lang.Integer">
			<column name="enable"  length="11" not-null="false" />
		</property>

		
		
	</class>
</hibernate-mapping>